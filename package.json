{
  "name": "ratpack-react",
  "version": "1.0.0",
  "description": "Boilerplate for Ratpack ReactJS project with hot code reloading",
  "scripts": {
    "clean": "rimraf dist",
    "webpack": "NODE_ENV=production webpack --config webpack.config.prod.js",
    "build": "npm run clean && npm run webpack",
    "start": "node devServer.js",
    "lint": "eslint src/main/resources"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gschrader/ratpack-react-boilerplate.git"
  },
  "keywords": [
    "react",
    "reactjs",
    "ratpack",
    "boilerplate",
    "hot",
    "reload",
    "live",
    "webpack"
  ],
  "author": "Glen Schrader <gschrader@gmail.com> (http://github.com/gschrader)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/gschrader/ratpack-react-boilerplate/issues"
  },
  "homepage": "https://github.com/gschrader/ratpack-react-boilerplate",
  "devDependencies": {
    "babel-core": "^5.4.7",
    "babel-eslint": "^3.1.9",
    "babel-loader": "^5.1.2",
    "css-loader": "^0.18.0",
    "file-loader": "^0.8.4",
    "url-loader": "^0.5.6",
    "extract-text-webpack-plugin": "^0.8.2",
    "react-hot-loader": "^1.2.7",
    "babel-plugin-react-transform": "^1.1.1",
    "eslint": "^1.3.1",
    "eslint-plugin-react": "^2.3.0",
    "express": "^4.13.3",
    "react-transform-catch-errors": "^1.0.0",
    "react-transform-hmr": "^1.0.0",
    "redbox-react": "^1.0.1",
    "rimraf": "^2.4.3",
    "webpack": "^1.9.6",
    "webpack-dev-middleware": "^1.2.0",
    "webpack-hot-middleware": "^2.0.0"
  },
  "dependencies": {
    "babel": "^5.8.23",
    "react": "^0.14.0",
    "react-dom": "^0.14.0",
    "jquery": "^2.1.4",
    "bootstrap": "^3.2.0",
    "react-bootstrap": "^0.27.2"
  }
}
